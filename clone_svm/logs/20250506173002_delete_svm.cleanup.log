export_policy:
  name: mypol
  rules:
  - allow_device_creation: true
    allow_suid: false
    anonymous_user_id: 65534
    chown_mode: restricted
    client_match: 0.0.0.0/0
    protocol:
    - nfs3
    ro_rule:
    - sys
    rule_index: 2
    rw_rule:
    - sys
    super_user_security:
    - sys

